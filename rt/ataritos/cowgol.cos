# 32-bit multiply: d0 = d0*d1. Uses d2, d3.
&X _mul4
``:
	move.w d1, d2
	mulu.w d0, d2
	move.l d1, d3
	swap d3
	mulu.w d0, d3
	swap d3
	clr.w d3
	add.l d3, d2
	swap d0
	mulu.w d1, d0
	swap d0
	clr.w d0
	add.l d2, d0
	rts

# 32-bit unsigned divide.
&X _divremu4
``:
	rts

# 32-bit signed divide.
&X _divrems4
``:
	rts

