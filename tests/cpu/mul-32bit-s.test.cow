var mtwo: int32 := -2;
var mone: int32 := -1;
var zero: int32 := 0;
var one: int32 := 1;
var two: int32 := 2;
var four: int32 := 4;
var twofivesix: int32 := 256;
var threehundred: int32 := 300;

sub yes() print(": yes\r"); end sub;
sub no() print(": no\r"); end sub;

print("two*two==4");            if (two*two) == 4            then yes(); else no(); end if;
print("threehundred*two==600"); if (threehundred*two) == 600 then yes(); else no(); end if;
print("twofivesix*two==512");   if (twofivesix*two) == 512   then yes(); else no(); end if;
print("mone*two=mtwo");         if (mone*two) == mtwo        then yes(); else no(); end if;
