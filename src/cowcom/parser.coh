include ".obj/parser.coh";

sub ParserInit()
	yyerrcnt := -1;
	yytos := &yystack[0];
	yystack[0].stateno := 0;
	yystack[0].major := 0;
	yystackend := &yystack[YYSTACKDEPTH];
end sub;

sub yy_pop_parser_stack()
	yytos := yytos - @bytesof yyStackEntry;
	yy_destructor(yytos.major, &yytos.minor);
end sub;

sub ParserDeinit()
	while yytos != &yystack[0] loop
		yy_pop_parser_stack();
	end loop;
end sub;

