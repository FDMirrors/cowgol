typedef string is [uint8];

record Keyword is
	name: string;
	id: uint8;
	value: Arith;
end record;

record Token is
	number @at(0): Arith;
	string @at(0): string;
end record;

var inputFilename: string;
var outputFilename: string := "output.bin";
var listingFilename: string;
var inputFile: FCB;
var outputFile: FCB;
var listingFile: FCB;
var lineno: uint16 := 1;


