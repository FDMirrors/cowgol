extern sub putc(c: uint8) := "putchar";

sub print(s: [uint8])
        loop
                var c := [s];
                if c == 0 then
                        return;
                end if;
                putc(c);
                s := s + 1;
        end loop;
end sub;

sub yes() print("yes\n"); end sub;
sub no() print("no\n"); end sub;

