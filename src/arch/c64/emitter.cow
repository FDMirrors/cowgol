sub arch_emit_header()
    var header: uint8[14];
    zero_memory(&header as [int8], header@bytes);
    header[0] := 0x01;
    header[1] := 0x08;
    header[2] := 12;
    header[3] := 8;
    header[6] := 158; # SYS
    header[7] := '2';
    header[8] := '0';
    header[9] := '6';
    header[10] := '1';
    file_seek(outfd, 0);
    file_putblock(outfd, &header as [int8], header@bytes);
end sub;
