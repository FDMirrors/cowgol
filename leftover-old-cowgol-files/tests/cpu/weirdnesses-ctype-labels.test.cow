const WHITESPACE := 0;
const ALPHABETIC := 4;

sub classify_char_type(c: uint8): (ctype: uint8)
    if c == ' ' then
        ctype := WHITESPACE;
    elseif (c >= 'a') or (c >= 'A') or (c == '_') then
        ctype := ALPHABETIC;
    end if;
end sub;

sub yes() print("yes\n"); end sub;

if classify_char_type('q') == ALPHABETIC then yes(); end if;
