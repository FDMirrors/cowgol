sub yes() print(": yes\n"); end sub;
sub no()  print(": no\n");  end sub;

sub test1()
    var valu8: uint8 := 0x01;
    var vals8: int8 := 0x10;
    var valns8: int8 := 0xfe;

    print("valu8 as uint8");    if (valu8 as uint8)    ==       0x01 then yes(); else no(); end if;
    print("vals8 as uint8");    if (vals8 as uint8)    ==       0x10 then yes(); else no(); end if;
    print("valns8 as uint8");   if (valns8 as uint8)   ==       0xfe then yes(); else no(); end if;
    print("valu8 as int8");     if (valu8 as int8)     ==       0x01 then yes(); else no(); end if;
    print("vals8 as int8");     if (vals8 as int8)     ==       0x10 then yes(); else no(); end if;
    print("valns8 as int8");    if (valns8 as int8)    ==       0xfe then yes(); else no(); end if;
    print("valu8 as uint16");   if (valu8 as uint16)   ==     0x0001 then yes(); else no(); end if;
    print("vals8 as uint16");   if (vals8 as uint16)   ==     0x0010 then yes(); else no(); end if;
    print("valns8 as uint16");  if (valns8 as uint16)  ==     0xfffe then yes(); else no(); end if;
    print("valu8 as int16");    if (valu8 as int16)    ==     0x0001 then yes(); else no(); end if;
    print("vals8 as int16");    if (vals8 as int16)    ==     0x0010 then yes(); else no(); end if;
    print("valns8 as int16");   if (valns8 as int16)   ==     0xfffe then yes(); else no(); end if;
    print("valu8 as uint32");   if (valu8 as uint32)   == 0x00000001 then yes(); else no(); end if;
    print("vals8 as uint32");   if (vals8 as uint32)   == 0x00000010 then yes(); else no(); end if;
    print("valns8 as uint32");  if (valns8 as uint32)  == 0xfffffffe then yes(); else no(); end if;
    print("valu8 as int32");    if (valu8 as int32)    == 0x00000001 then yes(); else no(); end if;
    print("vals8 as int32");    if (vals8 as int32)    == 0x00000010 then yes(); else no(); end if;
    print("valns8 as int32");   if (valns8 as int32)   == 0xfffffffe then yes(); else no(); end if;
end sub;
test1();

