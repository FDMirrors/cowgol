include "cowgol.coh";
include "_framework.coh";

interface Interface(in1: uint8, in2: uint32): (out1: uint32, out2: uint8);

sub Impl1 implements Interface is
	out1 := in2;
	out2 := in1;
end sub;

sub Impl2 implements Interface is
	out1 := 1;
	out2 := 2;
end sub;

var a: Interface := Impl2;

var i8: uint8;
var i32: uint32;
(i32, i8) := Impl1(1, 2);

